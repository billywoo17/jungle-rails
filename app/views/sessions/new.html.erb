<section class="user-new">

  <header class="page-header">
    <h1>Login</h1>
  </header>
<div class="row">
<%= form_tag '/login' do %>

 <div class="form-group">

        <div class="form-group">  
          Email
          <%= text_field_tag :email %>
        </div>

        <div class="form-group">
          Password
         <%= password_field_tag :password %>
        </div>               
        <%= submit_tag 'Create Account', class: 'btn btn-primary' %>
      <% end %>
    </div>
  </div>
</section>